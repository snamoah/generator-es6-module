{
  "name": "<%= _.slugify(appname) %>",
  "description": "",
  "version": "0.0.1",
  "author": "<%= creatorName %> <<%= email %>>",
  "repository": {
    "type": "git",
    "url": "git://github.com/<%= githubUser %>/<%= _.slugify(appname) %>.git"
  },
  "bugs" : "http://github.com/<%= githubUser %>/<%= _.slugify(appname) %>/issues",
  "engines" : {
    "node" : ">= 4.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.2.0"
  },
  "peerDependencies": {
  },
  "optionalDependencies": {
  },
  "devDependencies": {
    "babel-runtime": "^6.1.4",
    "babel-cli": "^6.1.4",
    "babel-preset-es2015": "^6.1.4",
    "babel-plugin-syntax-async-functions": "^6.1.18",
    "babel-plugin-transform-regenerator": "^6.2.0",
    "babel-eslint": "^5.0.0",
    "eslint": "^1.10.3",
    "glob": "^6.0.1",
    "tape": "^4.2.2"
  },
  "scripts": {
    "prepublish": "npm run compile",
    "test": "npm run compile && babel-node --presets es2015 test/harness.js test/**/*.js",
    "compile": "babel --optional selfContained --out-dir dist lib/**.js",
    "lint": "eslint lib"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "syntax-async-functions",
      "transform-regenerator"
    ]
  },
  "main": "./dist/lib"
}
