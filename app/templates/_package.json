{
  "name": "<%= _.slugify(appname) %>",
  "description": "",
  "version": "0.0.1",
  "author": "<%= creatorName %> <<%= email %>>",
  "repository": {
    "type": "git",
    "url": "git://github.com/<%= githubUser %>/<%= _.slugify(appname) %>.git"
  },
  "bugs" : "http://github.com/<%= githubUser %>/<%= _.slugify(appname) %>/issues",
  "engines" : {
    "node" : ">= 4.0.0"
  },
  "dependencies": {
    "babel-runtime": "^6.3.19",
    "babel-regenerator-runtime": "^6.3.13"
  },
  "devDependencies": {
    "babel-cli": "^6.1.4",
    "babel-preset-es2015": "^6.1.4",
    "babel-plugin-syntax-async-functions": "^6.1.18",
    "babel-plugin-transform-regenerator": "^6.4.4",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-eslint": "^4.0.0",
    "eslint": "^1.10.3",
    "istanbul": "^0.4.1",
    "tape": "^4.2.2"
  },
  "scripts": {
    "prepublish": "npm run compile",
    "test": "npm run compile && babel-node --presets es2015 node_modules/.bin/tape test/*.js",
    "compile": "babel --optional selfContained --out-dir dist lib/**.js",
    "lint": "eslint lib",
    "cover": "npm run compile && babel-node node_modules/.bin/istanbul cover node_modules/.bin/tape -- test/*.js"
  },
  "main": "./dist/lib"
}
